class months
    {
        //Varibles 
        private int mouthID;
        private int maximumtemp;
        private int mimimumtemp;
        private int numofdayofairfrost;
        private int millimetersofrain;
        private int hoursofsunshine;

        //constructor 
        public months(string themonthID, string themaximumtemp, string themimimumtemp,
                      string thenumofdayofairfrost, string themillmetersofrain,
                      string thehoursofsunshine)
        {
            mouthID = themonthID;
            maximumtemp = themaximumtemp;
            mimimumtemp = themimimumtemp;
            numofdayofairfrost = thenumofdayofairfrost;
            millimetersofrain = themillmetersofrain;
            hoursofsunshine = thehoursofsunshine;            
        }
        //Setters
        Public string setmonth(inmonth) 
        MonthId = inmonth
        
        Public string setmaxtemp(inmaxtemp) 
       Maxtemp = inmaxtemp
       
       Public string setmintemp(inmintemp) 
       Mintemp = inmintemp
       
       Public string numofdayairfrost(innumdayofairfrost) 
       Numofdayofairfrost = innumofdayofairfrost
       
       Publics string millimetersofrain(inmillimetersofrain) 
       millimeters of rain = inmillimetersofrain
       
       Public string hoursofsunshine(inhoursofsunshine) 
       hours of sunshine = inhoursofsunshine


    }
