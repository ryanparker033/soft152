 
 class month
 private int mouthID;
        private int maximumtemp;
        private int mimimumtemp;
        private int numofdayofairfrost;
        private int millimetersofrain;
        private int hoursofsunshine;


        //constructor 
        public months(int themonthID, int themaximumtemp, int themimimumtemp,
                      int thenumofdayofairfrost, int themillmetersofrain,
                      int thehoursofsunshine)

        {
            mouthID = themonthID;
            maximumtemp = themaximumtemp;
            mimimumtemp = themimimumtemp;
            numofdayofairfrost = thenumofdayofairfrost;
            millimetersofrain = themillmetersofrain;
            hoursofsunshine = thehoursofsunshine;
        }
        
        class Year
    {
        private int YearID;
        private string description;
        private int allmonths;


        //constructor 
        public Year(int theYearID, string thedescription, int theallmonths)
        {
           YearID = theYearID;
            description = thedescription;
            allmonths =  theallmonths;
        }
    }
